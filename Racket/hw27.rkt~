(define a
  (lambda (testValue)
    (or (and (> testValue -1) (<= testValue 3)) (and (>= testValue 4) (< testValue 6)))
    )) ; end of lambda and define
; tests
(display (a 2))
(display "...... should be #t") (newline)
(display (a 0))
(display "...... should be #t") (newline)
(display (a 6))
(display "...... should be #f") (newline)
(display (a 4))
(display "...... should be #t") (newline)
(display "-----------------------") (newline)

